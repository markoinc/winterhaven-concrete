  <!-- FAQ Section -->
  <section class="py-16 lg:py-24">
    <div class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-gray-900 mb-4">Frequently Asked Questions</h2>
        <p class="text-lg text-gray-600">Common questions about concrete services in {city}</p>
      </div>
      <div class="space-y-6">
        {faqs.map((faq, index) => (
          <div class="bg-white border border-gray-200 rounded-xl p-6">
            <h3 class="font-bold text-gray-900 mb-3">{faq.question}</h3>
            <p class="text-gray-600">{faq.answer}</p>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Nearby Areas -->
  <section class="py-16 lg:py-24 bg-gray-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-gray-900 mb-4">Also Serving Nearby Communities</h2>
        <p class="text-lg text-gray-600">We provide concrete services throughout Central Florida, including these {county} County neighbors.</p>
